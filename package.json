{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Project template with React, Webpack",
  "main": "index.js",
  "scripts": {
    "test": "mocha './src/**/*.test.js' --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "start": "webpack-dev-server --mode development --config webpack.config.dev.js --inline",
    "build": "webpack --mode production --config webpack.config.prod.js",
    "dev": "parallelshell 'npm run test:watch' 'npm start'",
    "lint": "eslint src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.3.0",
    "barba.js": "^1.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.6.0",
    "file-loader": "^6.2.0",
    "mocha": "^5.2.0",
    "parallelshell": "^3.0.2",
    "path": "^0.12.7",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "bootstrap-select": "^1.13.10",
    "d3": "^5.8.0",
    "lodash": "^4.17.11",
    "react": "^16.6.0",
    "scrollmagic": "^2.0.6"
  }
}
